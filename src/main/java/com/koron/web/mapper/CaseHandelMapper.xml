<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.koron.web.mapper.CaseHandelMapper">
	<insert id="addCaseHandel" useGeneratedKeys="true" keyProperty="id" parameterType="com.koron.web.bean.CaseHandelBean">
	   insert into 
	   tblcasehandel (userid,caseid,remark,endtime) 
	   values(#{userid},#{caseid},#{remark},#{endtime})
	</insert>
</mapper>